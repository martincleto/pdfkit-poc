{
  "name": "pdfkit-poc",
  "version": "1.0.0",
  "description": "",
  "main": "script.js",
  "scripts": {
    "test": "jest --watch",
    "start": "node server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "pdfkit": "^0.10.0"
  },
  "nodemonConfig": {
    "ext": "js,json,html"
  },
  "jest": {
    "automock": false,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/__coverage__/**",
      "!**/node_modules/**",
      "!**/public/**"
    ],
    "coverageDirectory": "__coverage__",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "setupFiles": [
      "./setupTest.js"
    ],
    "verbose": true
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.2"
  }
}
